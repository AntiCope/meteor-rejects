{
  "schemaVersion": 1,
  "id": "meteor-rejects",
  "version": "${version}",

  "name": "Meteor Rejects Addon",
  "description": "An addon to Meteor Client that adds modules and commands that were too useless to be added to Meteor directly.",
  "authors": [
    "Cloudburst",
    "StormyBytes"
  ],
  "contact": {
    "issues": "https://github.com/C10udburst/meteor-rejects/issues",
    "sources": "https://github.com/C10udburst/meteor-rejects"
  },

  "license": "GPL-3.0",
  "icon": "assets/rejects/icon.png",

  "environment": "client",
  "entrypoints": {
    "meteor": [
      "cloudburst.rejects.MeteorRejectsAddon"
    ]
  },
  "mixins": [
    "meteor-rejects.mixins.json",
    "meteor-rejects-meteor.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.7.4",
    "minecraft": "1.16.x",
    "meteor-client": "*"
  }
}
